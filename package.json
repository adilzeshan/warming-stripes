{
  "name": "warming-stripes",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "deploy": "gh-pages -d dist",
    "publish": "npm run build && npm run deploy",
    "test": "jest --silent",
    "test:update": "jest --silent -u",
    "test:watch": "jest --silent --watch",
    "test:coverage": "jest --silent --coverage",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,json,css}\"",
    "lint": "eslint --fix --quiet \"src/**/*.{js,jsx}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adilzeshan/warming-stripes"
  },
  "bugs": {
    "url": "https://github.com/adilzeshan/warming-stripes/issues"
  },
  "keywords": [],
  "author": "Adil Zeshan",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^4.0.0",
    "gh-pages": "^2.0.1",
